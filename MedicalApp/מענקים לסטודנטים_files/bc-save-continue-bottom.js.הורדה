define(['app'], function (app) {
    'use strict'

    var injectParams = [];
    var bcSaveContinueBottom = function () {
        return {
            restrict: 'E',
            templateUrl: 'templates/components/SaveAndContinueBottom.html'
        };
    };

    bcSaveContinueBottom.$inject = injectParams;
    app.directive('bcSaveContinueBottom', bcSaveContinueBottom);
});
