        var listForMap =
          autofillObj.data.Payload.ChildrenDetails.lChildren.length > 0
            ? autofillObj.data.Payload.ChildrenDetails.lChildren
            : autofillObj.data.Payload.SiblingsDetails.lSiblings; // check if got childrens or siblings
